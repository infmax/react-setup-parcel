        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
        "lib": [
          "es2015",
          "dom"
        ]
    },
    "include": [
        "./src/**/*"
    ]
